<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="yxinfo.core.service.ou.dao.mapper.SysTicketMapperExtend" >
  <insert id="replace" parameterType="yxinfo.core.service.ou.dao.model.SysTicket">
    REPLACE INTO
        sys_ticket ( ticket, create_at, user_key, use_at )
    VALUES
        ( #{ticket}, #{createAt}, #{userKey}, #{useAt} )
  </insert>
</mapper>